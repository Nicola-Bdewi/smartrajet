{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": [
      "./env.d.ts"
    ],
    "forceConsistentCasingInFileNames": false,
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "my-expo-env.d.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
